{
  "name": "mmo",
  "version": "1.0.0",
  "main": "server/dist/index.js",
  "repository": "https://tim-ings@github.com/tim-ings/mmo.git",
  "author": "Tim <tim@tim-ings.com>",
  "license": "SEE LICENSE IN LICENSE",
  "private": true,
  "scripts": {
    "watch-ts-server": "tsc -w --p tsconfig.server.json -outDir dist/server",
    "watch-webpack-client": "yarn webpack --watch",
    "watch-node-server": "nodemon dist/server/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TS Server,WP Client,Node\" -c \"yello.bold, cyan.bold\" \"yarn run watch-ts-server\" \"yarn run watch-webpack-client\" \"yarn run watch-node-server\"",
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "clean": "rm -rf dist/*"
  },
  "dependencies": {
    "concurrently": "^5.0.2",
    "express": "^4.17.1",
    "fs": "^0.0.1-security",
    "global": "^4.4.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "ts-keycode-enum": "^1.0.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^13.1.2",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^3.4.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.2",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.2",
    "ts-loader": "^6.2.1",
    "tsc": "^1.20150623.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.7.4",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
