{
  "name": "mmo",
  "version": "1.0.0",
  "main": "server/dist/index.js",
  "repository": "https://tim-ings@github.com/tim-ings/mmo.git",
  "author": "Tim <tim@tim-ings.com>",
  "license": "SEE LICENSE IN LICENSE",
  "private": true,
  "scripts": {
    "build": "tsc",
    "watch-ts-server": "tsc -w --p tsconfig.server.json -outDir dist/server",
    "watch-webpack-client": "yarn webpack --watch",
    "watch-node-server": "nodemon dist/server/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TS Server,WP Client,Node\" -c \"yello.bold, cyan.bold\" \"yarn run watch-ts-server\" \"yarn run watch-webpack-client\" \"yarn run watch-node-server\"",
    "test": "mocha -r ts-node/register src/**/*.spec.ts"
  },
  "dependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^13.1.2",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "awesome-typescript-loader": "^5.2.1",
    "concurrently": "^5.0.2",
    "express": "^4.17.1",
    "fs": "^0.0.1-security",
    "global": "^4.4.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^2.0.2",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "ts-keycode-enum": "^1.0.6",
    "ts-loader": "^6.2.1",
    "tsc": "^1.20150623.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.7.4",
    "uws": "^100.0.1",
    "webpack": "^4.41.5"
  },
  "devDependencies": {
    "webpack-cli": "^3.3.10"
  }
}
